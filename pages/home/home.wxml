<!--pages/home/home.wxml-->
<view class='dcf1'>{{text}}</view>
<button bindtap="changeText" class='dcf'>Change normal data</button>
<view>{{num}}</view>
<button bindtap="changeNum" class='wyx'>Change normal num</button>
<view>{{array[0].text}}</view>
<button bindtap="changeItemInArray" class='dcf'>Change Array data</button>
<view>{{object.text}}</view>
<button bindtap="changeItemInObject" class='wyx'>Change Object data</button>
<view>{{newField.text}}</view>
<button bindtap="addNewField" class='dcf'>Add new data</button>
<view>{{name}}</view>
<button bindtap='changeName' class='wyx'>Change Name</button>
<button bindtap='goNextPage'>Go to next page</button>
